{
	"name": "revkeep",
	"description": "Independent physician advisory service software",
	"author": {
		"name": "Kyle Weishaupt",
		"email": "kyle.weishaupt@revkeepsoftware.com",
		"url": "https://revkeepsoftware.com"
	},
	"repository": {
		"type": "git",
		"url": "https://bitbucket.org/revkeep/revkeep.git"
	},
	"version": "0.17.0",
	"license": "UNLICENSED",
	"private": true,
	"scripts": {
		"api": "php ./bin/cake.php server -H 0.0.0.0",
		"start": "npm run watch",
		"build": "npm run production",
		"dev": "npm run development",
		"prod": "npm run production",
		"development": "mix",
		"watch": "mix watch",
		"watch-poll": "mix watch -- --watch-options-poll=1000",
		"hot": "mix watch --hot",
		"production": "mix --production",
		"docker-build": "docker build -t revkeep:latest .",
		"docker-run": "docker run -d -p 8080:80 --name=revkeep --env-file=config/docker.local.env revkeep:latest",
		"docker-remove": "docker stop revkeep && docker rm revkeep || exit 0",
		"docker-rebuild": "npm run docker-remove && npm run docker-build && npm run docker-run"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.1",
		"@fortawesome/free-solid-svg-icons": "^6.1",
		"@fortawesome/vue-fontawesome": "^2.0.8",
		"axios": "^1.1",
		"bootstrap": "^4.6",
		"bootstrap-vue": "^2.23",
		"browser-sync": "^2.26.13",
		"browser-sync-webpack-plugin": "^2.2.2",
		"chart.js": "^4.1",
		"cross-env": "^7.0.3",
		"inputmask": "^5.0.5",
		"jquery": "^3.5.1",
		"laravel-mix": "^6.0.0",
		"lodash": "^4.17.20",
		"moment": "^2.29.1",
		"popper.js": "^1.16.1",
		"sass": "^1.54",
		"sass-loader": "^13.0",
		"v-mask": "^2.2.4",
		"vee-validate": "^3.4.5",
		"vue": "^2.7",
		"vue-chartjs": "^5.1",
		"vue-loader": "^15.10",
		"vue-multiselect": "^2.1.6",
		"vue-router": "^3.6.4",
		"vue-template-compiler": "^2.7",
		"vue-typeahead-bootstrap": "^2.12.0",
		"vuedraggable": "^2.24.3",
		"vuex": "^3.6.0"
	}
}
